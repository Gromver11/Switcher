{
    "extends": ["airbnb-base","prettier"],
    "rules": {
        "prettier/prettier": ["error", {}, {"usePrettierrc": true }],
        "max-len": ["error", { "code": 260 }]
    },
    "env": {
        "jest": true,
        "browser": true
    },
    "root": true,
    "plugins": [
        "prettier"
        ]
}